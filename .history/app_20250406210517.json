{
  "expo": {
    "name": "SheShield",
    "slug": "she-shield",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sheshield.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to provide a secure and convenient login experience.",
        "NSLocationWhenInUseUsageDescription": "We need your location to show you on the map.",
        "NSLocationAlwaysUsageDescription": "We need your location to show you on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sheshield.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "scheme": "she-shield",
    "extra": {
      "eas": {
        "projectId": "bbf0beb8-8168-4a38-9dbf-d23d30d904dd"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-local-authentication",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SheShield to use your location."
        }
      ]
    ]
  }
}
